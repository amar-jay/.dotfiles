{
  "breadcrumbs.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.fontSize": 15,
  "editor.glyphMargin": false,
  "editor.overviewRulerBorder": false,
  "editor.minimap.enabled": false,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.formatOnSave": true,
  "editor.roundedSelection": false,
  "editor.lineNumbers": "relative",
  "editor.inlineSuggest.enabled": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.cursorBlinking": "solid",
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": true
  },
  "vim.enableNeovim": true,
  "vim.easymotion": true,
  "vim.leader": "<space>",
  "vim.neovimPath": "/home/linuxbrew/.linuxbrew/bin/nvim",
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.visual": "block-outline",
  "vim.cursorStylePerMode.replace": "line",
  "vim.cursorStylePerMode.visualblock": "block-outline",
  "vim.highlightedyank.enable": true,
  "vim.cursorStylePerMode.visualline": "block-outline",
  "vim.overrideCopy": true,
  "editor.insertSpaces": false,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "One Dark Pro",
  "window.zoomLevel": -1,
  "css.lint.unknownAtRules": "ignore",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.typegen.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.typegen.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    "*.mts": "${capture}.typegen.ts",
    "*.cts": "${capture}.typegen.ts"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.enableDebugLogs": true,
  "rapidapi.terminalLink.enabled": false,
  "Lua.telemetry.enable": true,
  "files.autoSave": "afterDelay",
  "editor.fontFamily": "'NovaMono', Consolas, 'Courier New', monospace",
  "grammarly.files.include": [
    "**/*.md",
    "**/*.txt"
  ],
  "git.enableSmartCommit": false,
  "git.confirmSync": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.hideClosureInitialization": false,
  "rust-analyzer.inlayHints.typeHints.hideNamedConstructor": false,
  "rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": false,
  "rust-analyzer.inlayHints.reborrowHints.enable": "never",
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.bindingModeHints.enable": false,
  "rust-analyzer.inlayHints.renderColons": false,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "never",
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "never",
  "rust-analyzer.inlayHints.closingBraceHints.minLines": 0,
  "rust-analyzer.completion.callable.snippets": "none",
  "rust-analyzer.semanticHighlighting.doc.comment.inject.enable": false,
  "rust-analyzer.procMacro.ignored": {},
}
